<% content_for(:title) do %>
  List of <%= @indexed_content.table_name.titlecase %>
<% end %>

<% content_for :page_content do %>
<div class="page-content-parent">
    <h1>List of all <%= @indexed_content.table_name.titlecase %> by created date:</h1>
  <div class="profile-feed">
  <div class="user-index-grid">
  <%= render partial: "shared/main/user_index_grid", collection: @indexed_content,
                                                     as: :result,
                                                     locals: { button_type: @button_type,
                                                     is_group: @is_group } %>
  <% end %>
  </div>
  </div>
</div>